<?xml version="1.0" encoding="UTF-8"?>
<entities>
	<entity class="de.xwic.sandbox.demoapp.model.entities.ICampaign" autoLoadProperties="true" history="false" defaultDisplayProperty="name">
		<title>${name}</title>
		
		<property name="name" maxLength="100" required="true" />

		<property name="type" picklistId="campaign.type">
		     <picklistentry key="simple" index="10">
		         <name lang="en">Simple</name>
		     </picklistentry>
			<picklistentry key="newsletter" index="20">
		         <name lang="en">Newsletter</name>
		    </picklistentry>
			<picklistentry key="marketing" index="30">
				 <name lang="en">Marketing</name>
			</picklistentry>
		</property>
	</entity>
</entities>
