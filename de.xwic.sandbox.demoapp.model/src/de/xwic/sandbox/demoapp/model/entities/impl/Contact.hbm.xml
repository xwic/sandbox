<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.xwic.sandbox.demoapp.model.entities.impl.Contact" table="DA_CONTACT">

		 <id name="id" type="long">
            <column name="CONTACT_ID" not-null="true"/>
            <generator class="native"/>
        </id>

		<version column="VERSION" name="version" type="long"/>

 		<property name="deleted" column="DELETED" not-null="true"/>
 
        <property name="downloadVersion" type="long">
        	<column name="DOWNLOAD_VERSION"/>
        </property>
        <property name="serverEntityId" column="SRVENTITY_ID" not-null="true" type="int"/>

		<property name="createdAt" type="timestamp">
			<column name="CREATED_AT" />
		</property>
		
		<property name="changed" type="boolean">
			<column name="CHANGED" />
		</property>
		<property name="createdFrom">
			<column name="CREATED_FROM" length="50" />
		</property>
		<property name="lastModifiedFrom">
			<column name="LASTMODIFIED_FROM" length="50" />
		</property>
		<property name="lastModifiedAt" type="timestamp">
			<column name="LASTMODIFIED_AT" />
		</property>
		<!-- Entity fields -->
      
        <many-to-one name="title" class="de.xwic.appkit.core.model.entities.impl.PicklistEntry" column="TITLE" not-null="false"
                     lazy="false"/>

		<property name="firstName">
			<column name="FIRST_NAME" length="50" />
		</property>

		<property name="middleName">
			<column name="MIDDLE_NAME" length="10" />
		</property>

		<property name="lastName">
			<column name="LAST_NAME" length="50" />
		</property>

		<property name="phone1">
			<column name="PHONE1" length="30" />
		</property>

		<property name="phone2">
			<column name="PHONE2" length="30" />
		</property>

		<property name="fax">
			<column name="FAX" length="20" />
		</property>

		<property name="mobile1">
			<column name="MOBILE1" length="20" />
		</property>

		<property name="mobile2">
			<column name="MOBILE2" length="20" />
		</property>

		<property name="webSite">
			<column name="WEB_SITE" length="255" />
		</property>

		<property name="email1">
			<column name="EMAIL1" length="255" />
		</property>

		<property name="email2">
			<column name="EMAIL2" length="255" />
		</property>

        <many-to-one name="country" class="de.xwic.appkit.core.model.entities.impl.PicklistEntry" column="COUNTRY_ID" not-null="false"
                     lazy="false"/>

		<property name="state">
			<column name="STATE" length="255" />
		</property>

		<property name="city">
			<column name="CITY" length="255" />
		</property>

		<property name="zip">
			<column name="ZIP" length="20" />
		</property>
		
		<property name="address1">
			<column name="ADDRESS1" length="255" />
		</property>

		<property name="address2">
			<column name="ADDRESS2" length="255" />
		</property>
		
		<property name="notes" column="NOTES" type="materialized_clob"/>

		<property name="displayName">
			<column name="DISPLAY_NAME" />
		</property>

        <many-to-one name="company" class="de.xwic.sandbox.demoapp.model.entities.impl.Company" column="COMPANY"/>
        <!-- End entity fields -->
		
	</class>
</hibernate-mapping>
