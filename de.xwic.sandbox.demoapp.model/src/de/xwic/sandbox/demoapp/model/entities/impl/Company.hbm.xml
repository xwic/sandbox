<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.xwic.sandbox.demoapp.model.entities.impl.Company" table="DA_COMPANY">

		 <id name="id" type="int">
            <column name="COMPANY_ID" not-null="true"/>
            <generator class="native"/>
        </id>

		<version column="VERSION" name="version" type="long"/>

 		<property name="deleted" column="DELETED" not-null="true"/>
 
        <property name="downloadVersion" type="long">
        	<column name="DOWNLOAD_VERSION"/>
        </property>
        <property name="serverEntityId" column="SRVENTITY_ID" not-null="true" type="int"/>

		<property name="createdAt" type="timestamp">
			<column name="CREATED_AT" />
		</property>
		
		<property name="changed" type="boolean">
			<column name="CHANGED" />
		</property>
		<property name="createdFrom">
			<column name="CREATED_FROM" length="50" />
		</property>
		<property name="lastModifiedFrom">
			<column name="LASTMODIFIED_FROM" length="50" />
		</property>
		<property name="lastModifiedAt" type="timestamp">
			<column name="LASTMODIFIED_AT" />
		</property>
		<!-- Entity fields -->
      
        <many-to-one name="type" class="de.xwic.appkit.core.model.entities.impl.PicklistEntry" column="TYPE_ID" not-null="false"
                     lazy="false"/>

		<property name="name">
			<column name="NAME" length="100" />
		</property>

		<property name="phone1">
			<column name="PHONE1" length="30" />
		</property>

		<property name="fax">
			<column name="FAX" length="30" />
		</property>

		<property name="webSite">
			<column name="WEB_SITE" length="255" />
		</property>

		<property name="email1">
			<column name="EMAIL1" length="255" />
		</property>

		<property name="email2">
			<column name="EMAIL2" length="255" />
		</property>

        <many-to-one name="country" class="de.xwic.appkit.core.model.entities.impl.PicklistEntry" column="COUNTRY_ID" not-null="false"
                     lazy="false"/>

		<property name="state">
			<column name="STATE" length="255" />
		</property>

		<property name="city">
			<column name="CITY" length="255" />
		</property>

		<property name="zip">
			<column name="ZIP" length="20" />
		</property>
		
		<property name="address1">
			<column name="ADDRESS1" length="255" />
		</property>

		<property name="address2">
			<column name="ADDRESS2" length="255" />
		</property>


		<property name="notes" column="NOTES" type="materialized_clob"/>

        <many-to-one name="segment" class="de.xwic.appkit.core.model.entities.impl.PicklistEntry" column="SEGMENT_ID" not-null="false" lazy="false"/>

		<set name="relationship" table="DA_COMPANY_RELATIONSHIP">
			<key column="COMPANY_ID"/>
			<many-to-many class="de.xwic.appkit.core.model.entities.impl.PicklistEntry" column="RELATIONSHIP_ID"/>
		</set>

		<property name="annualRevenue" column="ANNUAL_REVENUE" />
		<property name="numberOfEmployees" column="NO_EMPLOYEES" />

		<!-- End entity fields -->
		
	</class>
</hibernate-mapping>
